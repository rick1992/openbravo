		<![CDATA[SELECT ao.name as organizacion ,mp.value cod_producto ,mp.name as producto, COALESCE(cut.name , cu.name) as um ,		COALESCE((		select spc.pricelist from SSA_Product_Currprice_V  spc		inner join m_pricelist mpl on spc.m_pricelist_id=mpl.m_pricelist_id		where mpl.issopricelist='Y' and mpl.c_currency_id='308' and spc.m_product_id=mp.m_product_id),0.00) as p_lista_pen ,		COALESCE((		select spc.pricelist from SSA_Product_Currprice_V  spc		inner join m_pricelist mpl on spc.m_pricelist_id=mpl.m_pricelist_id		where mpl.issopricelist='Y' and mpl.c_currency_id='100' and spc.m_product_id=mp.m_product_id),0.00) as p_lista_usd		 from m_product mp 		inner join ad_org ao on mp.ad_org_id=ao.ad_org_id		inner JOIN c_uom cu on mp.c_uom_id=cu.c_uom_id		inner join c_uom_trl cut on cu.c_uom_id=cut.c_uom_id		where		mp.ad_client_id in ('0','AF645935B14444CA8AD4A94FE6B2AF68') and 		mp.ad_org_id in ('F4CC51B76C4147019B0B88923623FBE2','BB3B411621084BC1AB8F3348896C23D0','7C50FE793FF247B89C4728C150D32803','4096DA159A6644648D47EE30B55F1795','3D64A6C76D214C31B43C58B8FF84CE50','233DC0B8A69F4826956CA9B23AB796B8') 				order by 1, 2, 3 limit 1]]>
