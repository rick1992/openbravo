	<queryString>
		<![CDATA[select ppc.ssa_projprop_contract_id as ssa_projprop_contract_id,

ppc.datedoc,
ppc.documentno,

bp.name as bpname,
bp.taxid,

loc.address1 || coalesce(' ' || loc.address2, '') || coalesce(' - ' || loc.city, '')
|| coalesce(' - '|| (select name from c_region r where r.c_region_id = loc.c_region_id), '')  as address,


(select i.phone from ssa_contact_information i where i.c_bpartner_id = bp.c_bpartner_id and i.isactive='Y' limit 1) as phone,
(select i.phone2 from ssa_contact_information i where i.c_bpartner_id = bp.c_bpartner_id and i.isactive='Y' limit 1) as phone2,
(select i.email from ssa_contact_information i where i.c_bpartner_id = bp.c_bpartner_id and i.isactive='Y' limit 1) as email,

coalesce(sr.name, '')as salesrep,
(select i.phone from ssa_contact_information i where i.c_bpartner_id=sr.c_bpartner_id and i.isactive='Y' limit 1) as salrepphone,
ppc.isfirstvisit,

coalesce(
(select c_project_id from c_project where c_project_id = (select t.c_project_id from
(select l.c_project_id, min(l.created) as created from ssa_projprop_contract_line l
where l.ssa_projprop_contract_id = ppc.ssa_projprop_contract_id
group by l.c_project_id) as t order by t.created limit 1)), 'EMPTY_ID') as "MV_PROJECT1_ID",

coalesce(
(select c_project_id from c_project where c_project_id = (select t.c_project_id from
(select l.c_project_id, min(l.created) as created from ssa_projprop_contract_line l
where l.ssa_projprop_contract_id = ppc.ssa_projprop_contract_id
group by l.c_project_id) as t order by t.created limit 1 offset 1)), 'EMPTY_ID') as "MV_PROJECT2_ID",

org.ad_org_id as "ORG_ID",
inf.Your_Company_Document_Image as "ORG_IMAGE_ID",

oloc.address1 || coalesce(' ' || oloc.address2, '') || coalesce(' - ' || oloc.city, '') || coalesce(' - '|| (select name from c_region r where r.c_region_id = oloc.c_region_id), '')  as orgaddress,

(select value from scr_combo_item where scr_combo_item_id = ppc.cboitem_buildingtype_id) as buildtype

from ssa_projprop_contract ppc

join c_bpartner bp on ppc.c_bpartner_id = bp.c_bpartner_id
left join c_bpartner_location bpl on ppc.c_bpartner_location_id = bpl.c_bpartner_location_id
left join c_location loc on bpl.c_location_id = loc.c_location_id

left join ad_user sr on ppc.salesrep_id = sr.ad_user_id
,
ad_org org
join ad_orgtype t using (ad_orgtype_id)
join ad_orginfo inf on org.ad_org_id = inf.ad_org_id

left join c_location oloc on inf.c_location_id = oloc.c_location_id

where ppc.ssa_projprop_contract_id in ($P{DOCUMENT_ID})
and ad_isorgincluded(ppc.ad_org_id, org.ad_org_id, ppc.ad_client_id)<>-1
and(t.IsLegalEntity='Y' or t.IsAcctLegalEntity='Y')
and ppc.isactive = 'Y'

limit 1

--]]>
