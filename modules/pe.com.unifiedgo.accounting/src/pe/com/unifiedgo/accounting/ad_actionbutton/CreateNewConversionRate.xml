<?xml version="1.0" encoding="UTF-8"?>
<REPORT>
    <template file="CreateNewConversionRate.html"/>     
    <PARAMETER id="paramDirectory" name="directory" default=""/>
    <PARAMETER id="paramLanguage" name="paramLanguage" default=""/>
    <PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
    <PARAMETER id="processDescription" name="description" replaceCharacters="htmlHelp" default=""/>
    <PARAMETER id="processHelp" name="help" replaceCharacters="htmlHelp" default=""/>
    <PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"/>
    <PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""/>
    <PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""/>    
    <PARAMETER id="fieldReceipt" name="isReceipt" attribute="value"/>
    
    <PARAMETER id="fieldOrg" name="orgId" attribute="value"/>
    <PARAMETER id="fieldcConversionRateId" name="cConversionRateId" attribute="value"/>
    <PARAMETER id="fieldWindowId" name="windowId" attribute="value"/>
    <PARAMETER id="fieldTabId" name="tabId" attribute="value"/>
 
    <PARAMETER id="paramConvrateDate" name="ConvrateDate" attribute="value"/>
    <PARAMETER id="paramConvrateDate" name="ConvrateDatedisplayFormat" attribute="displayformat" replace="xx"/>
    <PARAMETER id="paramConvrateDate" name="ConvrateDatesaveFormat" attribute="saveformat" replace="yy"/>
    
    <PARAMETER id="paramExchangeRateUSDPurch" name="ExchangeRateUSDPurch" attribute="value" format="generalQtyRelation"/>
    <PARAMETER id="paramExchangeRateUSDSales" name="ExchangeRateUSDSales" attribute="value" format="generalQtyRelation"/>
    <PARAMETER id="paramExchangeRateEURSales" name="ExchangeRateEURSales" attribute="value" format="generalQtyRelation"/>
    <PARAMETER id="paramExchangeRateEURPurch" name="ExchangeRateEURPurch" attribute="value" format="generalQtyRelation"/>
    <PARAMETER id="paramUSDExchangeRateEURSales" name="USDExchangeRateEURSales" attribute="value" format="generalQtyRelation"/>
    <PARAMETER id="paramUSDExchangeRateEURPurch" name="USDExchangeRateEURPurch" attribute="value" format="generalQtyRelation"/>
    
 </REPORT>
