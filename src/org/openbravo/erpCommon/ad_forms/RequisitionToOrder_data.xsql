  <SqlMethod name="linesToOrderAllOrg" type="preparedStatement" return="Multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
      <![CDATA[
        SELECT a.M_PRODUCTFINAL_ID as M_PRODUCT_ID, null as M_ATTRIBUTESETINSTANCE_ID, LOCKQTY, a.DESCRIPTION, 
               (SELECT C_UOM_ID FROM M_PRODUCT WHERE M_PRODUCT_ID = a.M_PRODUCTFINAL_ID) AS C_UOM_ID,
                QUANTITYORDER, null as M_PRODUCT_UOM_ID,
          a.M_REQUISITIONLINE_ID, '' AS C_ORDERLINE_ID,
          COALESCE(M_PRODUCTPRICE.PRICELIST, LOCKPRICE) AS PRICELIST,
          COALESCE(M_PRODUCTPRICE.PRICELIMIT, LOCKPRICE) AS PRICELIMIT,
          COALESCE(M_PRODUCTPRICE.PRICESTD, LOCKPRICE) AS PRICESTD,
          CASE WHEN M_Pricelist.istaxincluded = 'Y' then C_GET_NET_PRICE_FROM_GROSS(C_GetTax(a.M_PRODUCTFINAL_ID, to_date(?), ?, ?, ?, ?, null, 'N'), 
                                                                                    ROUND(COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD) * LOCKQTY,
                                                                                         (SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)) , 
                                                                                    ROUND(COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD) * LOCKQTY,
                                                                                         (SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)), 
                                                                                    (SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?), LOCKQTY) 
                                                ELSE ROUND(COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD),(SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)) END AS PRICEACTUAL,
          CASE WHEN COALESCE(M_PRODUCTPRICE.PRICELIST, a.PRICELIST, LOCKPRICE) = 0 THEN 0 ELSE
                    ROUND((COALESCE(M_PRODUCTPRICE.PRICELIST, a.PRICELIST, LOCKPRICE) - COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD))/
                           COALESCE(M_PRODUCTPRICE.PRICELIST, a.PRICELIST, LOCKPRICE)*100,
                    (SELECT STDPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)) END AS DISCOUNT,
          C_GetTax(a.M_PRODUCTFINAL_ID, to_date(?), ?, ?, ?, ?, null, 'N') AS TAX,
          CASE WHEN LOCKQTY < (a.QTY - a.ORDEREDQTY) THEN 'N' ELSE 'Y' END AS TO_CLOSE,
          CASE WHEN M_Pricelist.istaxincluded = 'Y' THEN ROUND(COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD),(SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)) ELSE 0 END AS GROSS_UNIT,
          CASE WHEN M_Pricelist.istaxincluded = 'Y' THEN ROUND(COALESCE(LOCKPRICE, M_PRODUCTPRICE.PRICESTD)*LOCKQTY,(SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?)) ELSE 0 END AS GROSS_AMT,
          a.needbydate, M_REQUISITION.documentno as INFOREQ
          FROM ( SELECT CASE WHEN ad_get_org_le_bu(M_REQUISITIONLINE.AD_ORG_ID,'LE')  <> ?  THEN                      
                        COALESCE((SELECT p.M_Product_id  
                                    FROM m_product p                                  
                                   WHERE ad_org_id = ?                                     
                                     AND value = (SELECT pr.EM_Scr_Internalcode FROM M_PRODUCT pr WHERE pr.M_PRODUCT_ID = M_REQUISITIONLINE.m_product_id)),M_REQUISITIONLINE.m_product_id)                       
                          ELSE M_REQUISITIONLINE.m_product_id END AS M_PRODUCTFINAL_ID,
                          M_REQUISITIONLINE.*
                 FROM M_REQUISITIONLINE 
                 WHERE 1=1
                  
          ) a  INNER JOIN M_REQUISITION ON a.M_REQUISITION_ID = M_REQUISITION.M_REQUISITION_ID
               LEFT JOIN M_PRODUCTPRICE
                                         ON a.M_PRODUCTFINAL_ID = M_PRODUCTPRICE.M_PRODUCT_ID
                                            AND M_PRODUCTPRICE.M_PRICELIST_VERSION_ID = ?
                               LEFT JOIN m_pricelist_version on M_PRODUCTPRICE.M_PRICELIST_VERSION_ID = m_pricelist_version.M_PRICELIST_VERSION_ID
                               LEFT JOIN m_pricelist on m_pricelist_version.m_pricelist_id = m_pricelist.m_pricelist_id
        
	    GROUP BY a.M_PRODUCTFINAL_ID, a.M_ATTRIBUTESETINSTANCE_ID, a.LOCKQTY, a.DESCRIPTION, a.C_UOM_ID, a.QUANTITYORDER, 
	    a.M_PRODUCT_UOM_ID, a.M_REQUISITIONLINE_ID, M_PRODUCTPRICE.PRICELIST, a.PRICELIST, a.LOCKPRICE,
	    M_PRODUCTPRICE.PRICELIMIT, M_PRODUCTPRICE.PRICESTD, M_Pricelist.istaxincluded, a.needbydate,
            a.QTY, a.ORDEREDQTY, SUPPLIERNOTES, M_REQUISITION.documentno
        ORDER BY MIN(a.LINE), a.M_PRODUCTFINAL_ID, a.M_ATTRIBUTESETINSTANCE_ID, SUPPLIERNOTES, 
                 ROUND(COALESCE(a.LOCKPRICE, M_PRODUCTPRICE.PRICESTD),(SELECT PRICEPRECISION FROM C_CURRENCY WHERE C_CURRENCY_ID = ?))
        ]]></Sql>
