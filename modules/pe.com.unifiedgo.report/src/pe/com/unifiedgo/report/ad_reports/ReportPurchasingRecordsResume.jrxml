	<queryString>
		<![CDATA[SELECT fa.em_sco_regnumber nreg,i.c_invoice_id,i.dateinvoiced fec_emi,f.duedate fec_venc,
        (CASE  WHEN (dt.name='AP Invoice' and i.em_sco_isimportation='N') THEN '01' WHEN (dt.name='AP Invoice' and i.em_sco_isimportation='Y') THEN '50' WHEN dt.name='AP Credit Memo' THEN '07' else '' end) as tdcomp,
        (CASE em_sco_isimportation WHEN 'Y' THEN  dua.sco_duanumber WHEN 'N' THEN i.em_scr_physical_documentno ELSE '' END ) serie, ci.code bptd,
        bp.taxid bpdocid,bp.name bpname, i.grandtotal imptotal ,rf.dateinvoiced refdate,rf.tipodoc reftdcomp,rf.nroserie refseriep,
        i.docstatus,'' bi1,'' igv1,'' bi2,'' igv2,'' bi3,'' igv3,'' atrib, '' currate, '' taxtotal , '' taxvalue, i.c_currency_id,'' nref_det,'' fec_emi_det
        FROM c_invoice i
        JOIN fin_payment_schedule f on f.c_invoice_id=i.c_invoice_id
        JOIN c_doctype dt on i.c_doctype_id=dt.c_doctype_id
        JOIN c_bpartner bp on i.c_bpartner_id=bp.c_bpartner_id
        LEFT JOIN sco_dua dua on i.em_sco_dua_id=dua.sco_dua_id
         LEFT JOIN (SELECT i2.c_invoice_id,i2.dateinvoiced,
                    (CASE  WHEN (dt2.name='AP Invoice' and i2.em_sco_isimportation='N') THEN '01' WHEN (dt2.name='AP Invoice' and i2.em_sco_isimportation='Y') THEN '50' WHEN dt2.name='AP Credit Memo' THEN '07' else '' end) as tipodoc,
                    (CASE i2.em_sco_isimportation WHEN 'Y' THEN  dua2.sco_duanumber WHEN 'N' THEN i2.em_scr_physical_documentno ELSE '' END ) nroserie
                    FROM c_invoice i2
                    JOIN c_doctype dt2 on i2.c_doctype_id=dt2.c_doctype_id
                      LEFT JOIN sco_dua dua2 on i2.em_sco_dua_id=dua2.sco_dua_id) rf on rf.c_invoice_id=i.em_sco_invoiceref_id
        LEFT JOIN scr_combo_item ci on ci.scr_combo_item_id=bp.em_scr_combo_item_id
	JOIN (SELECT DISTINCT em_sco_regnumber,record_id from fact_acct where isactive='Y' and ad_table_id='318') fa on i.c_invoice_id=fa.record_id
        WHERE
        i.docstatus IN ('CO' ,'VO')
        AND dt.name IN ( 'AP Invoice','AP Credit Memo' )
        AND i.issotrx='N'
order by tdcomp,fec_emi,i.created]]>
	</queryString>
