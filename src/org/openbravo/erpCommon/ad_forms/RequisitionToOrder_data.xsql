  <SqlMethod name="selectNoPriceAllOrg" type="preparedStatement" return="Multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
      <![CDATA[
      
        SELECT b.m_product_id as M_PRODUCT_ID,  
               AD_COLUMN_IDENTIFIER(to_char('M_Product'), to_char(b.M_PRODUCT_ID), ?) AS PRODUCT 
         FROM (
                SELECT DISTINCT CASE WHEN a.ad_org_id <> ? THEN
                      COALESCE((SELECT M_Product_id 
                                  FROM m_product 
                                 WHERE ad_org_id = ?  
                                   AND value = (SELECT EM_Scr_Internalcode FROM M_PRODUCT WHERE M_PRODUCT_ID = a.m_product_id) 
                               ),a.m_product_id)
                       ELSE a.m_product_id END AS M_PRODUCT_ID       
                  FROM 
				( SELECT DISTINCT ad_get_org_le_bu(AD_ORG_ID,'LE') as ad_org_id,  M_PRODUCT_ID
				       FROM M_REQUISITIONLINE
				   WHERE LOCKPRICE IS NULL          
				   AND 1=1  
				) a
            ) b
        WHERE
		NOT EXISTS ( SELECT 1 FROM M_PRODUCTPRICE 
		                     WHERE  b.M_PRODUCT_ID = M_PRODUCTPRICE.M_PRODUCT_ID                           
		                     AND M_PRODUCTPRICE.M_PRICELIST_VERSION_ID = ?) 
          ]]></Sql>
