	<queryString>
		<![CDATA[SELECT
	cy. YEAR AS ANIO,
	CASE EXTRACT (MONTH FROM cp.enddate)
WHEN 1 THEN
	'ENERO'
WHEN 2 THEN
	'FEBRERO'
WHEN 3 THEN
	'MARZO'
WHEN 4 THEN
	'ABRIL'
WHEN 5 THEN
	'MAYO'
WHEN 6 THEN
	'JUNIO'
WHEN 7 THEN
	'JULIO'
WHEN 8 THEN
	'AGOSTO'
WHEN 9 THEN
	'SETIEMBRE'
WHEN 10 THEN
	'OCTUBRE'
WHEN 11 THEN
	'NOVIEMBRE'
WHEN 12 THEN
	'DICIEMBRE'
END AS MES,
 COALESCE (
	SUBSTRING (
		fa.em_sco_regnumber
		FROM
			0 FOR (
				POSITION ('-' IN fa.em_sco_regnumber)
			)
	),
	'--'
) AS PROCESO,
 COALESCE (
	(
		SELECT
			sa.areaname
		FROM
			sco_area sa
		WHERE
			sa.areacode = COALESCE (
				SUBSTRING (
					fa.em_sco_regnumber
					FROM
						0 FOR (
							POSITION ('-' IN fa.em_sco_regnumber)
						)
				),
				'--'
			)
		LIMIT 1
	),
	''
) AS DESCRIPCIONPROCESO,
 fa.acctvalue,
 fa.acctdescription,
 cp.enddate,
 CASE
WHEN fa.record_id = 'NORECORD' THEN
	'Y'
ELSE
	'N'
END AS ESMIGRACION,
 SUM (fa.amtacctdr) AS debe,
 SUM (fa.amtacctcr) AS haber,
 SUM (fa.amtacctdr) - SUM (fa.amtacctcr) AS total,
 '' AS idorganizacion,
 '' AS idperiodo,
 '' AS periodo,
 '' AS fechainicial,
 '' AS fechafinal,
 '' AS idanio
FROM
	fact_acct fa
INNER JOIN c_period cp ON fa.c_period_id = cp.c_period_id
INNER JOIN c_year cy ON cp.c_year_id = cy.c_year_id
WHERE
	fa.ad_client_id IN (
		'0',
		'AF645935B14444CA8AD4A94FE6B2AF68'
	)
AND fa.ad_org_id IN (
	'A823605601994B658319133117D23344',
	'67C43FEDE8F44A67B5F676FD942DCA81',
	'26C51A26367B4E97BEE8C6EC6ECD6F8B',
	'2161C6B6075C4D41AC6CD8F365A4B206'
)
AND cy. YEAR = '2016'
AND 1 = 1
GROUP BY
	1,
	2,
	3,
	4,
	5,
	6,
	7,
	8
ORDER BY
	1,
	7,
	3]]>
	</queryString>
