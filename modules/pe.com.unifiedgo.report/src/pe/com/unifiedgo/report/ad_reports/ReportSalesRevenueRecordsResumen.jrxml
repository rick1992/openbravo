	<queryString>
		<![CDATA[SELECT i.documentno nreg,i.c_invoice_id,i.dateinvoiced fec_emi,f.duedate fec_venc,
        (CASE  dt.name WHEN 'AR Invoice' THEN '01' WHEN 'AR Ticket' THEN '03' WHEN 'AR Credit Memo' THEN '07' WHEN 'Return Material Sales Invoice' THEN '07' WHEN 'AR Dedit Memo' THEN '08' else '' end) as tdcomp,
        i.em_scr_physical_documentno serie, bp.taxid bpdocid,bp.name bpname, igv.taxbaseamt base, igv.taxamt t_igv,  i.grandtotal imptotal ,rf.dateinvoiced refdate,rf.tipodoc reftdcomp,rf.nroserie refseriep,
        i.docstatus,ci.code bptd,'' exon,'' inaf, '' atrib, '' currate, '' taxtotal , '' taxvalue, i.c_currency_id
        FROM c_invoice i
        JOIN fin_payment_schedule f on f.c_invoice_id=i.c_invoice_id
        JOIN c_doctype dt on i.c_doctype_id=dt.c_doctype_id
        JOIN c_bpartner bp on i.c_bpartner_id=bp.c_bpartner_id
        LEFT JOIN (SELECT  it.c_invoice_id,it.taxamt,it.taxbaseamt FROM c_invoicetax it  JOIN c_tax t on it.c_tax_id=t.c_tax_id WHERE em_sco_specialtax='SCOIGV')  igv on igv.c_invoice_id=i.c_invoice_id
        LEFT JOIN (SELECT i2.c_invoice_id,i2.dateinvoiced,(CASE  dt.name WHEN 'AR Invoice' THEN '01' WHEN 'AR Ticket' THEN '03' WHEN 'AR Credit Memo' THEN '07' WHEN 'Return Material Sales Invoice' THEN '07' WHEN 'AR Dedit Memo' THEN '08' else '' end) as tipodoc,
                    i2.em_scr_physical_documentno nroserie
                    FROM c_invoice i2
                    JOIN c_doctype dt on i2.c_doctype_id=dt.c_doctype_id ) rf on rf.c_invoice_id=i.em_sco_invoiceref_id
        LEFT JOIN scr_combo_item ci on ci.scr_combo_item_id=bp.em_scr_combo_item_id
        WHERE
        i.docstatus IN ('CO' ,'VO')
        AND dt.name IN ( 'AR Invoice','AR Ticket','AR Credit Memo','AR Dedit Memo','Return Material Sales Invoice' )
        AND i.issotrx='Y'
order by tdcomp,fec_emi,i.created]]>
	</queryString>
