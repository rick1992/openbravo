	<queryString>
		<![CDATA[select prop.ssa_project_property_id, proj.name as projectname,
       (select lsttrl.name from ad_ref_list lst, ad_ref_list_trl lsttrl where lst.ad_ref_list_id=lsttrl.ad_ref_list_id and lst.value=proj.projectstatus) as projectstatus,
       (select lst.name from ad_reference r, ad_ref_list lst where r.ad_reference_id=lst.ad_reference_id and r.name='SSA_ProjectProperty_Type' and lst.value=prop.type) as type,
       prop.name as propertyname, prop.number,
       (select cur.cursymbol from c_currency cur where cur.c_currency_id=prop.c_currency_id) as cursymbol, prop.totalamount as price,
       prop.totalarea, prop.roofedtarea, prop.notbuiltarea,
       (select lst.name from ad_reference r, ad_ref_list lst where r.ad_reference_id=lst.ad_reference_id and r.name='SSA_ProjectProperty_Status' and lst.value=prop.docstatus) as status
from c_project proj
inner join ssa_project_property prop on proj.c_project_id=prop.c_project_id
--where proj.c_project_id=? --(param *) por proyecto
--and p.ad_client_id=? (*)
--and ad_isorgincluded(?, p.ad_org_id, p.ad_client_id) > -1 (param*) por org
order by prop.number, prop.created

limit 1]]>
	</queryString>
