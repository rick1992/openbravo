	<queryString>
		<![CDATA[select f.*,
vida_util-vida_cumplida as vida_x_cumplir,
depreciacion_acumulada + depreciacion_ejercicio as depreciacion_historica,
depreciacion_acumulada + depreciacion_ejercicio as depreciacion_acumulada_inflasion,
saldo_inicial-(depreciacion_acumulada + depreciacion_ejercicio) as activo_neto
 from
(select

aa.ad_org_id,ao.name,

aag.name as clase

, (select cev.value from a_asset_acct aaa2
left join c_validcombination cvc on aaa2.em_sco_assets_acct=cvc.c_validcombination_id
inner join c_elementvalue cev on cvc.account_id=cev.c_elementvalue_id
where aaa2.a_asset_id=aa.a_asset_id limit 1 ) as codigo_cuenta

,aa.value as codigo_activo

,aa.name as activo

,( case when  aa.amortizationcalctype = 'PE' then 'porcentaje'
	when  aa.amortizationcalctype = 'TI' then 'tiempo' end) as tipodepreciacion

,( case when  aa.assetschedule = 'YE' then 'anual'
	when  aa.assetschedule = 'MO' then 'mensual' end) as tiempo

,to_char(aa.amortizationstartdate,'dd/mm/yyyy') as fecha_inicio

,to_char(aa.datepurchased,'dd/mm/yyyy') as fecha_adquisicion

,(
case 	when  aa.amortizationcalctype = 'PE' then round( aa.annualamortizationpercentage/12,2)
	when  aa.amortizationcalctype = 'TI' then
		(select case when aa.assetschedule = 'YE' then round((aaml.amortization_percentage)/12,2)
			when aa.assetschedule = 'MO' then (aaml.amortization_percentage) end
		from a_amortizationline aaml
		inner join a_amortization am2 on aaml.a_amortization_id=am2.a_amortization_id
		where aaml.a_asset_id=aa.a_asset_id   and am2.startdate<=('2018-01-01') and am2.enddate>=('2018-01-01') limit 1)
	end
) as depreciacion

,(case
	when  aa.amortizationcalctype = 'TI' then coalesce( aa.uselifeyears*12,coalesce(aa.uselifemonths , 0))
	when  aa.amortizationcalctype = 'PE' then
	(select count(aaml.a_amortizationline_id) from a_amortizationline aaml
		inner join a_amortization am2 on aaml.a_amortization_id=am2.a_amortization_id
		where aaml.a_asset_id=aa.a_asset_id)*12
end
) as vida_util

,((SELECT (DATE_PART('year', '2018-01-01'::date) - DATE_PART('year', aa.amortizationstartdate)) * 12 +
              (DATE_PART('month', '2018-01-01'::date) - DATE_PART('month', aa.amortizationstartdate)))+1
) as vida_cumplida

,(select coalesce(arlt.name,coalesce(arl.name,'')) from ad_ref_list arl
left join ad_ref_list_trl arlt on arl.ad_ref_list_id=arlt.ad_ref_list_id
where  arl.value=aa.amortizationtype) as tipo_regresion

,1 as cantidad

,aa.Amortizationvalueamt as saldo_inicial

,0.00 as adquisiciones_adicionales

,1.0000 as mejoras

,0.00 as retiro_bajas

,aa.assetvalueamt as valor_historico

,aa.assetvalueamt as valor_ajustado

,aa.a_asset_id
,(
select sum(aaml.amortizationamt) from a_amortizationline aaml
inner join a_amortization am2 on aaml.a_amortization_id=am2.a_amortization_id
where aaml.a_asset_id=aa.a_asset_id and am2.startdate<=('2018-01-01'::date-interval '11 month') --
and (case when aa.assetschedule = 'MO'  then
case when am2.enddate<=('2018-01-01'::date-interval '11 month') then true else false end
else true end )
)+aa.Depreciatedpreviousamt as depreciacion_acumulada

,(select sum(aaml.amortizationamt) from a_amortizationline aaml
inner join a_amortization am2 on aaml.a_amortization_id=am2.a_amortization_id
where aaml.a_asset_id=aa.a_asset_id  and am2.startdate>=('2018-01-01'::date-interval '11 month') and am2.startdate<='2018-01-01') as depreciacion_ejercicio

,0.00 as depreciacion_bajas

from a_asset aa
inner join ad_org ao on aa.ad_org_id=ao.ad_org_id
left join a_asset_group aag on aa.a_asset_group_id=aag.a_asset_group_id
--where ao.ad_org_id='26C51A26367B4E97BEE8C6EC6ECD6F8B'
) as  f]]>
